{
    "src_folders": [
        "tests"
    ],
    "webdriver": {
        "start_process": true,
        "server_path": "node_modules/.bin/chromedriver",
        "port": 9515
    },
    "selenium_server": {
        "selenium": {
            "start_process": true,
            "host": "localhost",
            "server_path": "./bin/selenium-server-standalone-4.0.0-alpha-1.jar",
            "cli_args": {
                "webdriver.chrome.driver": "./bin/chromedriver"
            }
        }
    },
    "test_settings": {
        "default": {
            "desiredCapabilities": {
                "browserName": "chrome"
            }
        }
    }
}